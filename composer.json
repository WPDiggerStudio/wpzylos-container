{
  "name": "wpdiggerstudio/wpzylos-container",
  "description": "PSR-11 compatible dependency injection container with auto-wiring for WPZylos framework",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "wpzylos",
    "container",
    "di",
    "dependency-injection",
    "psr-11",
    "autowiring",
    "ioc"
  ],
  "homepage": "https://github.com/WPDiggerStudio/wpzylos-container",
  "authors": [
    {
      "name": "WPDiggerStudio",
      "email": "diggerwp@gmail.com",
      "homepage": "https://github.com/WPDiggerStudio",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/WPDiggerStudio/wpzylos-container/issues",
    "source": "https://github.com/WPDiggerStudio/wpzylos-container",
    "docs": "https://wpzylos.com/docs/latest/packages/wpzylos-container"
  },
  "require": {
    "php": "^8.0",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.6 || ^10.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "WPZylos\\Framework\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WPZylos\\Framework\\Container\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyze": "phpstan analyze",
    "cs": "phpcs --standard=PSR12 src",
    "cs-fix": "phpcbf --standard=PSR12 src",
    "qa": ["@cs", "@analyze", "@test"]
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "funding": [
    {
      "type": "paypal",
      "url": "https://www.paypal.com/donate/?hosted_button_id=66U4L3HG4TLCC"
    }
  ]
}